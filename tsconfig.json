{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "ags/gtk4",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "paths": {
      "@components": [
        "./components"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@style": [
        "./style"
      ],
      "@style/*": [
        "./style/*"
      ]
    },
    "strict": true,
    "target": "ES2020"
  }
}